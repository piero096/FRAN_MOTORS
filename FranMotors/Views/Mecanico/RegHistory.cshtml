@{ 
    var moto = (Motocicleta)Model;
}

<div class="container">
    <h1>Registrar Historial</h1>
    <div class="mt-3">
        <form action="/mecanico/reghistory?id=@moto.Id&idcliente=@moto.IdCliente " method="post">
            <div>
                <input type="text" name="Detalle" />
            </div>
            <div class="mt-2">
                <button>Registrar</button>
            </div>
        </form>
    </div>
</div>